    "web-components",
    "tabs"
  ],
  "author": "Viet Ngoc",
  "license": "MIT",
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.40.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  }
}